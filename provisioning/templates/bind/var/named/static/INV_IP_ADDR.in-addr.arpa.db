$ORIGIN INV_IP_ADDR.in-addr.arpa.
$TTL 3600
@      IN SOA  ns1.OSE_DNS_DOMAIN. hostmaster.OSE_DNS_DOMAIN. (
                                2011112968 ; serial
                                60         ; refresh (1 minute)
                                15         ; retry (15 seconds)
                                1800       ; expire (30 minutes)
                                10         ; minimum (10 seconds)
                                )
;
@    IN   NS      ns1.OSE_DNS_DOMAIN.

